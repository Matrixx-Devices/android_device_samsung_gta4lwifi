on init
   # SKB IPTV requirement
   write /proc/sys/net/core/rmem_default 10485760
   write /proc/sys/net/core/rmem_max 10485760

on boot
   # SSWAP
   write /proc/sys/vm/swappiness 160
   #!/system/bin/sh
   write /dev/watchdog1 V
   write /dev/watchdog0 V
   write /dev/watchdog V

on property:ro.boot.em.model=*
    mount none /vendor/firmware/wlanmdsp_${ro.boot.em.model}.mbn /vendor/firmware/wlanmdsp.mbn bind
